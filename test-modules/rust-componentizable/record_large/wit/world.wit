package component:test-package;

interface env {
  record user-profile {
    id: u64,
    username: string,
    email: string,
    first-name: string,
    last-name: string,
    age: u32,
    country: string,
    city: string,
    is-verified: bool,
    postal-code: string,
    phone: string,
    account-balance: f64,
    loyalty-points: u32,
    subscription-tier: string,
    last-login-timestamp: u64,
    registration-date: string,
    tags: list<string>,
    purchase-history: list<u64>,
    preferences: list<string>,
  }

  process-profile: func(profile: user-profile) -> user-profile;
}

world my-world {
  import env;
  use env.{user-profile};
  
  export main: func(profile: user-profile) -> string;
}
